.list{
  padding:0 30rpx 30rpx 30rpx;
  border-bottom: 6px solid #F6F6F6;
}
.listTop{
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 10rpx 0;
  color: #ccc;
}
.listCenter{
  display: flex;
}
.boxText{
  font-size: 16px; 
}
.boxImage image{
  width: 150rpx;
  height: 150rpx;
  margin-right: 30rpx;
}
.end{
  display: flex;
  justify-content: center;
  padding-top: 35rpx;
  color: #ccc;
  font-size: 14px;
}