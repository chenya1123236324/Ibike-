<import src="../../../wxParse/wxParse.wxml"/>
<view>
  <text class="title">{{ latestDetail.article_title  }}</text>
  <view class="title-presentation">
    <text class="count">{{ latestDetail.count }}点击</text>
    <text class="author">作者：{{ latestDetail.author }}</text>
  </view>
  <template is="wxParse" data="{{wxParseData:article.nodes}}"/>

  <view class="footer">
    <button open-type="share" type="primary" size="default">分享</button>
  </view>
</view>

